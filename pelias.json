{
  "esclient": {
    "apiVersion": "2.x",
    "keepAlive": true,
    "requestTimeout": "120000",
    "hosts": [
      {
        "env": "dev",
        "protocol": "http",
        "host": "localhost",
        "port": 9200
      }
    ],
    "log": [{
      "type": "stdio",
      "level": [ "error", "warning" ]
    }]
  },
  "logger": {
    "level": "verbose"
  },
  "imports": {
    "openstreetmap": {
      "datapath": "/mnt/data/openstreetmap/",
      "leveldbpath": "/tmp",
      "adminLookup": true,
      "import": [{
        "type": { "node": "osmnode", "way": "osmway" },
        "filename": "finland-latest.osm.pbf"
      }]
    },
    "openaddresses": {
      "datapath": "/mnt/data/openaddresses/",
      "adminLookup": true
    },
    "whosonfirst": {
      "datapath": "/mnt/data/whosonfirst/wof_data"
    }
  },
  "schema": {
    "indexName": "pelias"
  }
}
